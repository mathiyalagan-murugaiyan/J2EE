<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>s1</servlet-name>
        <servlet-class>servlet.ServletOne</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s1</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>s2</servlet-name>
        <servlet-class>servlet.ServletTwo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s2</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>s3</servlet-name>
        <servlet-class>servlet.ServletThree</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s3</servlet-name>
        <url-pattern>/d1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>s4</servlet-name>
        <servlet-class>servlet.ServletFour</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/d2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>s5</servlet-name>
        <servlet-class>servlet.GenericServletOne</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s5</servlet-name>
        <url-pattern>/s5</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>s6</servlet-name>
        <servlet-class>servlet.GenericServletTwo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s6</servlet-name>
        <url-pattern>/s6</url-pattern>
    </servlet-mapping>
    <context-param>
        <param-name>DB-Name</param-name>
        <param-value>data</param-value>
    </context-param>
    <servlet>
        <servlet-name>c1</servlet-name>
        <servlet-class>servlet.ServletContext1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>c1</servlet-name>
        <url-pattern>/url1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>c2</servlet-name>
        <servlet-class>servlet.ServletContext2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>c2</servlet-name>
        <url-pattern>/url2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>d1</servlet-name>
        <servlet-class>servlet.Db1</servlet-class>
        <init-param>
            <param-name>database</param-name>
            <param-value>sys</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>d1</servlet-name>
        <url-pattern>/db</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>d2</servlet-name>
        <servlet-class>servlet.Db2</servlet-class>
        <init-param>
            <param-name>emp</param-name>
            <param-value>hari</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>d2</servlet-name>
        <url-pattern>/emp</url-pattern>
    </servlet-mapping>
</web-app>
